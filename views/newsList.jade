each news in data
	div(class="news" style='word-wrap: normal;')
		div(class="newsSubject")
			| #{news.F_SUBJECT}
		div(class="postDate")
			| #{news.F_POST_DT}
		div(class="newsMessage")
			| #{news.F_MESSAGE}
		if news.F_VIDEO_URL!=undefined
			a(href=`${news.F_VIDEO_URL}`,target="_blank")| #影片連結
	<hr>